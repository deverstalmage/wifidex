import React, {
  Component,
  View,
  Text,
} from 'react-native';

export default class StrategyList extends Component {
  constructor(props) {
    super(props);
  }

  render() {
    const { strategies } = this.props;

    const strategyList = strategies.map(strategy => {
      const { type: { name } } = type;
      return (
        <Text key={`type_${name}`}>{name}</Text>
      )
    }).reverse();

    return (
      <View style={{padding: 10}}>
        {strategyList}
      </View>
    );
  }
}